<?xml version="1.0"?>
<suite name="application-help">
  <class>solaris_app_help.SolarisAppHelpTest</class>
  <description>
    Tests that start and close the Help from application, verify the Help menu/button can invoke gnome-help
  </description>
  <case name="Invoke Dictionary Help">
    <method>testAppHelp</method>
    <description>It opens the Dictionary Help and closes the Help window.</description>
    <args>
      <appname>gnome-dictionary</appname>
      <windowname>frmDictionary</windowname>
      <helpwidget>mnuContents</helpwidget>
      <helpwindow>frmDictionaryManual</helpwindow>
      <closetype>menu</closetype>
      <closename>mnuClose</closename>
    </args>
  </case>
