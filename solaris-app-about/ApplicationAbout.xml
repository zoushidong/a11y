<?xml version="1.0"?>
<suite name="application-about">
  <class>solaris_app_about.SolarisAppAboutTest</class>
  <description>
    Tests that start and close the About dialog from application, verify the About menu/button can invoke About dialog
  </description>
  <case name="Invoke Dictionary About">
    <method>testAppAbout</method>
    <description>It opens the Dictionary About and closes the About window.</description>
    <args>
      <appname>gnome-dictionary</appname>
      <windowname>frmDictionary</windowname>
      <aboutwidget>mnuAbout</aboutwidget>
      <aboutwindow>dlgAboutDictionary</aboutwindow>
      <closetype>menu</closetype>
      <closename>mnuClose</closename>
    </args>
  </case>
